{
  "name": "cordova-code-swap",
  "version": "1.2.7",
  "description": "Allows fetching updates to js-part of app",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbjorge/cordova-code-swap.git"
  },
  "scripts": {
    "test": "mocha",
    "build": "run-s lint test build:clean build:js",
    "build:js": "babel src --out-dir build --plugins transform-object-assign --presets=es2015",
    "build:clean": "mkdir -p build && rm -r build/*",
    "lint": "eslint src/*",
    "ci": "nodemon --watch ./src --exec 'npm run build'",
    "ci-test": "nodemon --exec 'npm run test'"
  },
  "keywords": [],
  "author": "Joachim Bj√∏rge <jb@ground.no> (http://ground.no)",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "filenamify": "^1.2.1",
    "url-join": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2"
  }
}
