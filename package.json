{
  "name": "cordova-code-swap",
  "version": "2.0.5",
  "description": "Allows fetching updates to js-part of app",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbjorge/cordova-code-swap.git"
  },
  "scripts": {
    "test": "mocha test/index.js",
    "build": "run-s lint test build:clean build:js",
    "build:js": "babel ./src --out-dir ./dist --plugins transform-object-assign --presets=es2015",
    "build:clean": "mkdir -p ./dist && rm -r dist/*",
    "lint": "eslint src/* utils/* cli/*",
    "ci": "nodemon --watch ./src --exec 'npm run build'",
    "ci-test": "nodemon -e js,json,spec --exec 'npm run test' --watch ./src --watch ./utils"
  },
  "bin": {
    "ccs": "./cli/index.js"
  },
  "keywords": [],
  "author": "Joachim Bj√∏rge <jb@ground.no> (http://ground.no)",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "express": "^4.14.0",
    "figlet": "^1.2.0",
    "filenamify": "^1.2.1",
    "glob-watcher": "^3.1.0",
    "globby": "^6.1.0",
    "mkdirp": "^0.5.1",
    "socket.io": "^1.7.2",
    "url-join": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.1",
    "mocha": "^3.4.2",
    "mock-require": "^2.0.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
    "proxyquire": "^1.8.0"
  }
}
